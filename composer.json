{
    "name": "clvarley/bench",
    "type": "library",
    "description": "Trivial PHP benchmarking",
    "homepage": "https://github.com/clvarley/bench",
    "license": "MIT",
    "readme": "README.md",
    "authors": [
        {
            "name": "Conor Varley",
            "homepage": "https://github.com/clvarley",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.11"
    },
    "autoload": {
        "psr-4": {
            "Clvarley\\Bench\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Clvarley\\Bench\\Tests\\": "tests/"
        }
    }
}
